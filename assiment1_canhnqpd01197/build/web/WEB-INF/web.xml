<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <context-param>
        <param-name>javax.faces.PROJECT_STAGE</param-name>
        <param-value>Development</param-value>
    </context-param>
    <servlet>
        <servlet-name>Faces Servlet</servlet-name>
        <servlet-class>javax.faces.webapp.FacesServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>dangki</servlet-name>
        <servlet-class>servlet.dangki</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>xuatTT</servlet-name>
        <servlet-class>servlet.xuatTT</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>xuatSP</servlet-name>
        <servlet-class>servlet.xuatSP</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>dangnhap</servlet-name>
        <servlet-class>servlet.dangnhap</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>dangxuat</servlet-name>
        <servlet-class>servlet.dangxuat</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>thongtin</servlet-name>
        <servlet-class>servlet.thongtin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updateTT</servlet-name>
        <servlet-class>servlet.updateTT</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>doipass</servlet-name>
        <servlet-class>servlet.doipass</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>giohang</servlet-name>
        <servlet-class>servlet.giohang</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Faces Servlet</servlet-name>
        <url-pattern>/faces/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>dangki</servlet-name>
        <url-pattern>/dangki</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>xuatTT</servlet-name>
        <url-pattern>/xuatTT</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>xuatSP</servlet-name>
        <url-pattern>/xuatSP</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>dangnhap</servlet-name>
        <url-pattern>/dangnhap</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>dangxuat</servlet-name>
        <url-pattern>/dangxuat</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>thongtin</servlet-name>
        <url-pattern>/thongtin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updateTT</servlet-name>
        <url-pattern>/updateTT</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>doipass</servlet-name>
        <url-pattern>/doipass</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>giohang</servlet-name>
        <url-pattern>/giohang</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>faces/index.xhtml</welcome-file>
    </welcome-file-list>
</web-app>
